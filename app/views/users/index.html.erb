<% if flash[:error] %>
  <%= flash[:error ]%>
<% end %>

<table>
<tr>
    <td>Name</td>
    <td>Email</td>
    <td>Phone</td>
</tr>
<% @users.each do |user| %>
<tr>
    <td><%= link_to(user.name, user_url(user))   %></td>
    <td><%= user.email %></td>
    <td><%= user.phone %></td>
</tr>
<% end %>
</table>